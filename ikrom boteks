#include <stdio.h>
#include <iostream>
using namespace std;

int main()
{
    int a= 6;
	    
    int sum = 0;

    for (int i = 1; i < 6; i++) {
    
        if (a % i == 0) {
            sum = sum + i;
        }
    }
	    
    if (a == sum) {
        cout<<"true";
    } else {
        cout<<"false";
    }
    
    return 0;
}
